ALGORITHM checkpoint2

VAR

    T1 : ARRAY_OF INTEGER[];
    T2 : ARRAY_OF INTEGER[];
    i : INTEGER;
    sum : INTEGER := 0;
    n : INTEGER ; //lenghth array

BEGIN

    Write ("initialize first array"); 

    FOR i FROM 0 TO n DO
        Read(T1[i]);
    END_FOR

    FOR i FROM 0 TO n DO
        Read(T2[i]);
    END_FOR

    FOR i FROM 0 TO n DO
        IF (T1[i]==T2[i]) THEN 
        sum := sum + T1[i];
        ELSE
        Write (T1[i]); 
        END IF
        END FOR

END
